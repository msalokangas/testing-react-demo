{
  "name": "",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "main": "",
  "scripts": {
    "start": "node server.js",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test NODE_PATH=./src mocha --require babel-core/register --require ./testhelper.js \"src/**/*@(.spec.js)\"",
    "test:coverage": "nyc --reporter=lcov --reporter=text npm test",
    "test:watch": "npm run test -- --watch",
    "test:e2e": "./node_modules/.bin/nightwatch -e browserstack",
    "test:e2e:local": "./node_modules/.bin/nightwatch",
    "test:core": "cross-env NODE_ENV=test NODE_PATH=./src mocha --require babel-core/register \"src/core/**/*@(.spec.js)\"",
    "dist:integration": "webpack -p --config ./webpack/dist.config.js --progress --env=integration",
    "nyc:report": "nyc report --reporter=text-lcov > coverage.lcov && ./node_modules/.bin/codecov",
    "lint": "eslint ./src",
    "clean": "rimraf dist/*"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "testhelper.js",
      "**/config/**",
      "**/constants/**"
    ],
    "sourceMap": false,
    "instrument": false
  },
